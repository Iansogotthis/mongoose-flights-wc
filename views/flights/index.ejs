<h2 style="text-align: center;">Movie List</h2>
<table>
  <thead>
    <tr >
      <th>Arline Name</th>
      <th>Airport Name</th>
      <th>Flight Number</th>
      <th>Flight Date</th>
      
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
      <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
      </tr>
    <% }); %>
  </tbody>
</table>